{
  "name": "Mirror an image vertically",
  "fws": [
    {
      "fw_id": 1,
      "name": "Mirror vertically",
      "spec": {
        "_tasks": [
          {
            "_fw_name": "CommandLineTask",
            "inputs": ["image to flip"],
            "outputs": ["flipped image"],
            "command_spec": {
                "command": ["convert", "-flip"],
                "flipped image": {
                    "target": {
                        "type": "path",
                        "value": "/tmp"
                    }
                },
                "image to flip": {
                    "source": "image to flip"
                }
            }
          }
        ],
        "image to flip": {
            "type": "path",
            "value": "piece-1.png"
        }
      }
    }
  ],
  "links": {},
  "metadata": {}
}
