{
 "name": "CommandLineTask demo",
 "fws": [
  {
   "fw_id": 1,
   "name": "Run a command and store the result",
   "spec": {
    "_tasks": [
     {
      "_fw_name": "CommandLineTask",
      "inputs": ["input string"],
      "outputs": ["output file"],
      "command_spec": {
        "command": ["echo"],
        "input string": {
          "source": "input string"
        },
        "output file": {
          "source": {
            "type": "stdout"
          },
          "target": {
            "type": "path",
            "value": "/tmp"
          }
        }
      }
     }
    ],
    "input string": {
      "type": "data",
      "value": "Hello world!"
    }
   }
  }
 ],
 "links": {},
 "metadata": {}
}
