{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/FWorker",
  "FWorker": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "query": {
        "type": "string",
        "default": "{}"
      },
      "category": {
        "oneOf": [
          {
            "type": "null"
          },
          {
            "type": "string"
          },
          {
            "type": "array",
            "items": {
              "type": [
                "string",
                "null"
              ]
            }
          }
        ]
      },
      "env": {
        "type": "object"
      }
    }
  }
}
